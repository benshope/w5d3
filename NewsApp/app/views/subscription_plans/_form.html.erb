<%= form_for @subscription_plan do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %><br>
	<%= f.label :price %>
	<%= f.number_field :price %><br>
	<%= f.label :weekly %>
	<%= f.number_field :weekly %><br>
	<%= f.hidden_field :newspaper_id, value: params[:newspaper_id] %>
	<%= f.submit nil, :class => "btn" %>
<% end %>