<%= form_for @newspaper do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %><br>
	<%= f.label :editor %>
	<%= f.text_field :editor %><br><br>
	<%= f.label "Subscription" %><br>
	<%= f.fields_for :subscription_plans do |spf| %>
		<%= spf.label :name%>
		<%= spf.text_field :name %><br>
		<%= spf.label :price %>
		<%= spf.number_field :price %><br>
		<%= spf.radio_button :weekly, "0" %>
		<%= spf.radio_button :weekly, "1" %><br>
	<% end %>
	<%= f.submit %>
<% end %>